=ifs(f2=d2,"配偶",and(LEFT(f2,1)=LEFT(B2,1),MID(g2,7,4)-MID(C2,7,4)>50),"孙子女",and(LEFT(f2,1)=LEFT(B2,1),MID(g2,7,4)-MID(C2,7,4)>18),"子女",and(LEFT(f2,1)=LEFT(B2,1),MID(g2,7,4)-MID(c2,7,4)<-50),"祖父母",and(LEFT(f2,1)=LEFT(B2,1),MID(g2,7,4)-MID(c2,7,4)<-18),"父母",and(LEFT(f2,1)=LEFT(d2,1),MID(g2,7,4)-MID(e2,7,4)>50),"孙子女",and(LEFT(f2,1)=LEFT(d2,1),MID(g2,7,4)-MID(e2,7,4)>18),"子女",and(LEFT(f2,1)=LEFT(d2,1),MID(g2,7,4)-MID(e2,7,4)<-18),"岳父母或公婆",LEFT(f2,1)=LEFT(B2,1),"兄弟姐妹")
